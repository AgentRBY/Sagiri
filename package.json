{
  "name": "sagiri",
  "version": "2.0.0",
  "description": "A simple, lightweight and actually good JS wrapper for the SauceNAO API.",
  "author": "sr229",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/ClarityCafe#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ClarityCafe/Sagiri"
  },
  "bugs": {
    "url": "https://github.com/ClarityCafe/issues"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prebuild": "yarn lint && yarn test",
    "build": "rollup -c rollup.config.ts",
    "start": "yarn build -w",
    "prepublishOnly": "yarn build && yarn docs",
    "predocs": "yarn docs:clean && yarn lint && yarn docs:build",
    "docs": "jsdoc2md --template ./docs/template.hbs --files ./docsbuild/*.js --example-lang ts > ./README.md",
    "postdocs": "yarn docs:clean",
    "docs:clean": "rimraf ./docsbuild",
    "docs:build": "tsc -p ./tsconfig.docs.json",
    "copyfiles": "copyfiles -u 1 ./docs/example.js ./dist"
  },
  "keywords": [
    "hitorigoto",
    "sagiri",
    "saucenao",
    "simple",
    "easy",
    "api"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "dependencies": {
    "form-data": "^2.5.0"
  },
  "devDependencies": {
    "@favware/eslint-config": "^1.0.0",
    "@types/form-data": "^2.2.1",
    "@types/jest": "^24.0.15",
    "@types/node": "12.6.8",
    "common-tags": "^1.8.0",
    "copyfiles": "^2.1.1",
    "eslint": "^6.1.0",
    "jest": "^24.8.0",
    "jsdoc-to-markdown": "^5.0.0",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-progress": "^1.1.1",
    "rollup-plugin-terser": "5.1.1",
    "rollup-plugin-typescript2": "^0.22.1",
    "rollup": "^1.17.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "@types/form-data": "^2"
  },
  "files": [
    "dist"
  ]
}